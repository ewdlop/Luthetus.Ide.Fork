        <definition>static string Luthetus.Common.RazorLib.FileSystems.Models.PathHelper.GetAbsoluteFromAbsoluteAndRelative</definition>
        <argsstring>(IAbsolutePath absolutePath, string relativePathString, IEnvironmentProvider environmentProvider)</argsstring>
        <name>GetAbsoluteFromAbsoluteAndRelative</name>
        <qualifiedname>Luthetus.Common.RazorLib.FileSystems.Models.PathHelper.GetAbsoluteFromAbsoluteAndRelative</qualifiedname>
        <param>
          <type><ref refid="interface_luthetus_1_1_common_1_1_razor_lib_1_1_file_systems_1_1_models_1_1_i_absolute_path" kindref="compound">IAbsolutePath</ref></type>
